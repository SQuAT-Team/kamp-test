<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="edu.kit.ipd.sdq.kamp.wizard.category"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.uka.ipd.sdq.componentInternalDependencies.presentation.ComponentInternalDependenciesModelWizardID"
            name="%_UI_ComponentInternalDependenciesModelWizard_label"
            class="de.uka.ipd.sdq.componentInternalDependencies.presentation.ComponentInternalDependenciesModelWizard"
            category="edu.kit.ipd.sdq.kamp.wizard.category"
            icon="icons/full/obj16/ComponentInternalDependenciesModelFile.gif">
         <description>%_UI_ComponentInternalDependenciesModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="de.uka.ipd.sdq.componentInternalDependencies.presentation.ComponentInternalDependenciesEditorID"
            name="%_UI_ComponentInternalDependenciesEditor_label"
            icon="icons/full/obj16/ComponentInternalDependenciesModelFile.gif"
            extensions="componentinternaldependencies"
            class="de.uka.ipd.sdq.componentInternalDependencies.presentation.ComponentInternalDependenciesEditor"
            contributorClass="de.uka.ipd.sdq.componentInternalDependencies.presentation.ComponentInternalDependenciesActionBarContributor">
      </editor>
   </extension>

</plugin>
